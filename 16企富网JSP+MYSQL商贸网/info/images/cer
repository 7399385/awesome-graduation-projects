<script language=javascript>
function check(form)
{
  if(form.chrDomain.value==""){alert("请输入您想打印证书的域名!");form.chrDomain.focus();return(false);}
  var domain=form.chrDomain.value.toLowerCase();
  /*if(/^[0-9a-z-]{1,}\.{1,1}[0-9a-z]{2,}\.{0,1}[0-9a-z]{0,}/.test(domain)==false){
     alert("域名格式不正确!");form.chrDomain.focus();return(false);
  }*/
  if(domain.indexOf(".")==-1){ alert("请输入正确的域名!");form.chrDomain.focus();return(false);}
  var start=domain.lastIndexOf(".");
  var isChDomain=false;
  if(domain.charCodeAt(0)>127) isChDomain=true;
  var Suffix=domain.substring(start,domain.length);
  if(isChDomain) form.action="cer_ch.net"
  else if(Suffix==".cn"){
     form.action="cer_cn.net"
  }else{
     form.action="cer.net"
  }
}
</script>
